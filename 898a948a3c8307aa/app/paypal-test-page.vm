      <wix-payment-paypal configurations="main.configurations"></wix-payment-paypal>
    </div>
<!-- process-tags prefix('${staticsUrl}') -->
    <!-- build:js scripts/component-test-modules.js -->
    <script src="bower_components/jquery/jquery.js"></script>
